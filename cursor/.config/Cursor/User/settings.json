{
    "editor.fontFamily": "CaskaydiaMono Nerd Font",
    "editor.tabSize": 2,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.minimap.enabled": false,
    "git.ignoreMissingGitWarning": true,
    "editor.fontSize": 12,
    "editor.occurrencesHighlight": "off",
    "editor.selectionHighlight": false,
    "editor.suggestOnTriggerCharacters": true,
    "editor.tabCompletion": "on",
    // "editor.quickSuggestions": {
    //     "other": false,
    //     "comments": false,
    //     "strings": false
    // },
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "window.titleBarStyle": "custom",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "editor.inlineSuggest.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "editor.fontLigatures": false,
    "vim.handleKeys": {
        "<C-a>": true,
        "<C-f>": true,
        "<C-p>": false,
        "<C-w>": false,
        "<C-/>": false,
        "<C-c>": false,
        "<C-i>": false
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<Esc>",
                "l",
                "a"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<Esc>",
                "h",
                "a"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<C-a>"
            ],
            "after": [
                "g",
                "g",
                "V",
                "G"
            ]
        },
        {
            "before": [
                "2",
                "o"
            ],
            "after": [
                "o",
                "<Esc>",
                "o"
            ]
        },
        {
            "before": [
                "2",
                "O"
            ],
            "after": [
                "O",
                "<Esc>",
                "O"
            ]
        },
        {
            "before": [
                "y",
                "w"
            ],
            "after": [
                "y",
                "i",
                "w"
            ]
        },
        {
            "before": [
                "c",
                "w"
            ],
            "after": [
                "c",
                "i",
                "w"
            ]
        },
        {
            "before": [
                "d",
                "w"
            ],
            "after": [
                "d",
                "i",
                "w"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "."
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.copyLinesDownAction"
                }
            ]
        },
        {
            "before": [
                ","
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.copyLinesUpAction"
                }
            ]
        },
        {
            "before": [
                "w"
            ],
            "after": [
                "i",
                "w"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "i",
                "\t"
            ]
        }
    ],
    "editor.lineNumbers": "relative",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vim.argumentObjectClosingDelimiters": [
        ")",
        "]",
        "}"
    ],
    "vim.argumentObjectOpeningDelimiters": [
        "(",
        "[",
        "{"
    ],
    "vim.changeWordIncludesWhitespace": true,
    "vim.highlightedyank.enable": true,
    "vim.timeout": 500,
    "terminal.integrated.scrollback": 10000,
    "explorer.confirmPasteNative": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.editor.showTabs": "single",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "files.eol": "\n",
    "prisma.showPrismaDataPlatformNotification": false,
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "github.copilot.editor.enableAutoCompletions": false,
    "supermaven.allowGitignore": true,
    "[jsonc]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "window.customTitleBarVisibility": "auto",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "cursor.cpp.enablePartialAccepts": true,
    "svelte.enable-ts-plugin": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[yaml]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[dockercompose]": {
      "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "window.newWindowProfile": "my-profile",

    "diffEditor.renderSideBySide": false,
    "comments.thread.confirmOnCollapse": "never",
    "githubPullRequests.pullBranch": "never",
    "claudeCode.preferredLocation": "panel",
    


}
